%nav.hero-nav.ten.columns

  %ul.left.six.columns
    -@posts.each do |post|
      %li
        %span.infotags= post.title

  %ul.right.four.columns
    -@projects.each do |project|
      %li
        %span.infotags= project.title

%section.posts-container.fourteen.columns
  -@log.each do |log|
    %article.post
      %h3=log.title
      %p=log.content